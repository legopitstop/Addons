                "components": {
                    "minecraft:rotation": [
                        0,
                        90,
                        0
                    ]
                }
            },
            {
                "name": "direction=east",
                "condition": "q.block_property('const:direction') == 5",
                "components": {
                    "minecraft:rotation": [
                        0,
                        -90,
                        0
                    ]
                }
            }
        ],
        "events": {
            "const:place_direction": {
                "set_block_property": {
                    "const:direction": "(q.cardinal_facing_2d)"
                }
            },
            "const:message": {
                "run_command": {
                    "command": "say This block is experimental, It may not stay inside the addon."
                }
